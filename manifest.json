{
	"name": "Motivate Yourself",
	"version": "1.0",
  	"description": "Time to get some shit done.",
  	"manifest_version":2,
  	"browser_action": {
    	"default_icon": "heart.png",
    	"default_popup": "popup.html"
  	},
  	"background": {
  		"scripts": ["background.js"]
  	},
  	"permissions": [
  		"webRequest",
  		"webRequestBlocking",
  		"tabs",
  		"http://*/*", 
  		"https://*/*"
  	], 
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
